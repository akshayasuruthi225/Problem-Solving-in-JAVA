import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner o=new Scanner(System.in);
        int n=o.nextInt();
        if(n>0)
        System.out.println("Positive");
        else if(n<0)
            System.out.println("Negative");
        else
            System.out.println("Zero");
    }
}
