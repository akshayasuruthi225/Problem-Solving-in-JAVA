import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner o=new Scanner(System.in);
        int n=o.nextInt();
        for(int i=1;i<=n;i++)
        {
            for(int j=i;j<=n;j++)
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }

}
