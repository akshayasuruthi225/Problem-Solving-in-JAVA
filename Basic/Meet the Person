import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner o=new Scanner(System.in);
        int age=o.nextInt();
        float height=o.nextFloat();
        String name=o.next();
        System.out.printf("%s is %d years old and %.2f meters tall.",name,age,height);
    }
}
